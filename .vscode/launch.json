{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Flutter Web (Port 3000)",
            "request": "launch",
            "type": "dart",
            "deviceId": "web-server",
            "args": [
                "--web-port=3000"
            ],
            "serverReadyAction": {
                "pattern": "lib\\\\main.dart is being served at (http://localhost:3000)",
                "uriFormat": "%s",
                "action": "openExternally"
            }
        },
        {
            "name": "Flutter Web Debug (Simple Browser)",
            "request": "launch",
            "type": "dart",
            "deviceId": "web-server",
            "args": [
                "--web-port=3000"
            ]
        },
        {
            "name": "flutter (Debug)",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "flutter (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "flutter (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        }
    ]
}